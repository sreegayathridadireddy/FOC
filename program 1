#include<stdio.h>
#include<conio.h>
int main()
{
	float c,f,cn,fn;
	
	printf("Enter temperature in centigrade:");
	scanf("%f", &c);
	f=1.8*c+32;
	printf("Farenheit equivalent is :%.1f\n",f);
	printf("Enter temperature in farenheit:");
	scanf("%f", &fn);
	cn=(fn-32)/1.8;
	printf("Centigrade equivalent is :%.1f\n",cn);
	getch();
}
